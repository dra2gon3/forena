<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
	<head>
		<title>User logs</title>
        <frx:category>Drupal Administration</frx:category>
        <frx:options/> 
		<frx:parameters>
			<frx:parm id="name" require="1" label="User Name">3</frx:parm>
		</frx:parameters>
		<frx:parameters>
			<frx:parm id="search" require="0" label="Search">school</frx:parm>
		</frx:parameters>
		<frx:fields><frx:field id="name" link="" format="" format-string="" target="">Anonymous</frx:field><frx:field id="total" link="reports/DrupalUserLogs?name={name}" format="date" format-string="" target=""/><frx:field id="type" link="" format="" format-string="" target=""/><frx:field id="location" link="" format="" format-string="" target=""/><frx:field id="timestamp" link="" format="drupal_date_format" format-string="small" target=""/><frx:field id="details" link="admin/reports/event/{wid}" format="" format-string="" target="">Details</frx:field></frx:fields>
	<frx:docgen/></head>
	<body>
		<div frx:block="drupal/users_in_logs">
			<h4>Users Appearing in Logs</h4>
			<table>
				<thead>
					<tr><th width="200">User</th><th>Log Entries</th></tr>
				</thead>
				<tbody>
					<tr frx:foreach="*">
						<td>{name}</td>
						<td>{total}</td>
						<td/>
					</tr>
				</tbody>
			</table>			
		</div>
		<h4>Log Entries for {name}</h4>
		  <table frx:block="drupal/user_logs">
				<thead>
					<tr><th width="200px">Type</th><th width="200">Location</th><th>Timestamp</th></tr>
				</thead>
				<tbody>
					<tr frx:foreach="*">
						<td>{type}</td>
						<td>{location}</td>
						<td>{timestamp}</td>
						<td>{details}</td>
						<td/>
					</tr>
				</tbody>
		  </table>
	</body>
</html>
